{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "pnpm build:vercel",
    "installCommand": "pnpm install",
    "outputDirectory": "dist",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/vercel.js"
        }
    ]
}
